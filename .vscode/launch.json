{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Run API",
            "type": "debugpy",
            "request": "launch",
            "program": "api\run.py",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/api/.env.dev"
        }
    ]
}